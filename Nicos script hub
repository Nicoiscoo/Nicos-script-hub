local Tab = Window:MakeTab({
	Name = "Playerüòé",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

--[[
Name = <string> - The name of the tab.
Icon = <string> - The icon of the tab.
PremiumOnly = <bool> - Makes the tab accessible to Sirus Premium users only.
]]

local Section = Tab:AddSection({
	Name = "Tap to execute"
})

--[[
Name = <string> - The name of the section.
]]

Tab:AddButton({
	Name = "Superhuman (High JP and WS)",
	Callback = function()
      		-- Get the player character and humanoid
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")

-- Set walkspeed and jump power
humanoid.WalkSpeed = 200
humanoid.JumpPower = 200

  	end    
})

--[[
Name = <string> - The name of the button.
Callback = <function> - The function of the button.
]]

Tab:AddButton({
	Name = "Fly (Universal)",
	Callback = function()
      		loadstring(game:HttpGet("https://raw.githubusercontent.com/scripthubekitten/dahoodios/main/hoodcustomfly", true))()
  	end    
})

--[[
Name = <string> - The name of the button.
